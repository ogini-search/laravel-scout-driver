{
  "name": "ogini-search/laravel-scout-driver",
  "description": "Laravel Scout driver for OginiSearch - High-performance search engine integration",
  "type": "library",
  "license": "MIT",
  "version": "1.0.0",
  "keywords": [
    "laravel",
    "scout",
    "search",
    "ogini",
    "elasticsearch",
    "full-text-search",
    "indexing",
    "faceted-search"
  ],
  "homepage": "https://github.com/ogini-search/laravel-scout-driver",
  "support": {
    "issues": "https://github.com/ogini-search/laravel-scout-driver/issues",
    "docs": "https://github.com/ogini-search/laravel-scout-driver#readme",
    "source": "https://github.com/ogini-search/laravel-scout-driver",
    "email": "support@oginisearch.com"
  },
  "authors": [
    {
      "name": "Precious Atam",
      "email": "preciousatam@gmail.com",
      "homepage": "https://oginisearch.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "guzzlehttp/guzzle": "^7.0",
    "laravel/framework": "11.*",
    "laravel/scout": "^10.0",
    "orchestra/testbench": "9.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "nunomaduro/larastan": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "OginiScoutDriver\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OginiScoutDriver\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage",
    "analyse": "phpstan analyse",
    "release": "./scripts/release.sh",
    "release:patch": "./scripts/release.sh patch",
    "release:minor": "./scripts/release.sh minor",
    "release:major": "./scripts/release.sh major",
    "release:prerelease": "./scripts/release.sh prerelease",
    "check-updates": "php artisan ogini:check-updates",
    "validate-release": "composer validate && composer test && composer analyse"
  },
  "extra": {
    "laravel": {
      "providers": [
        "OginiScoutDriver\\OginiServiceProvider"
      ],
      "aliases": {
        "Ogini": "OginiScoutDriver\\Facades\\Ogini",
        "OginiUpdateChecker": "OginiScoutDriver\\Facades\\UpdateChecker"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "preferred-install": "dist"
  }
}
